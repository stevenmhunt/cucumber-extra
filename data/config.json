{
    "alwaysEnabled": true,
    "profiles": {
        "default": {
            "require": []
        }
    },
    "timeouts": {
        "global": 0
    },
    "steps": {
        "delay": {
            "before": 0,
            "after": 0
        },
        "retry": {
            "count": 0,
            "delay": 0,
            "backoff": 0
        }
    },
    "types": {
        "enabled": true,
        "supported": {
            "json": true,
            "keywords": true,
            "literals": true
        },
        "stripHiddenSpaces": true
    },
    "templates": {
        "enabled": true,
        "engine": "handlebars",
        "options": {
            "cacheCompiledTemplates": true,
            "noEscape": true
        }
    }
}